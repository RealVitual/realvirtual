@media screen and (max-width 1366px)
	.gTitle
		font-size 30px

@media screen and (max-width 1024px)
	.grilla
		width calc(100% - 40px)
	.gTitle
		font-size 28px
	.gParrafo
		font-size 15px

@media screen and (max-width 480px)
	.gParrafo
		font-size 14px
	.gTitle
		font-size 22px
	.grilla
		width calc(100% - 20px)