.b9
    padding 24px 0px


.b9Item
    height 140px
    padding 38px 24px
    border-radius 8px
    box-shadow: 0px 11px 30px 0px #0000000D
    width 32%
    margin-right 2%
    &:nth-child(4n)
        margin-right 0
.b9TextItem
    width calc(100% - 24px)
    padding-left 8px
    h4
        font-size 18px
        textg(t-500,normal,#828282)
    h5
        font-size 20px
        textg(t-500,normal,#17181B)